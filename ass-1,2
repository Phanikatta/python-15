#Ass-1
n=int(input())
l=list(map(int,input().split()))
maxx=max(l)
l.sort()
for i in range(n):
    if(l[i]==maxx):
        m=l[i-1]
        break
print(m)

#Ass-2
n=int(input())
l=list(map(int,input().split()))
m=max(l)
for i in range(1,m+1):
    if i not in l:
        print(i,end=' ')
    if i in l:
        continue
    else:
        print(i,end=' ')
